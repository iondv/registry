        var plugin_path = '<%=baseUrl%><%=module%>/js/plugins/';
        <% if (typeof locale == 'object') {%>
        $.fn.dataTable.moment("<%=locale.dateFormat%>");
        $.fn.dataTable.moment("<%=locale.dateTimeFormat%>");
        <% } %>
